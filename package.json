{
  "name": "prishonor-ts",
  "version": "1.0.0",
  "description": "Prishonor game - TypeScript/HTML5 Canvas implementation",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "sync:assets": "node scripts/sync-assets.js",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:e2e": "playwright test",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.{ts,css}\""
  },
  "dependencies": {},
  "devDependencies": {
    "@types/node": "latest",
    "@typescript-eslint/eslint-plugin": "latest",
    "@typescript-eslint/parser": "latest",
    "eslint": "latest",
    "prettier": "latest",
    "typescript": "latest",
    "vite": "latest",
    "vitest": "latest",
    "playwright": "latest"
  },
  "keywords": [
    "game",
    "typescript",
    "canvas",
    "html5"
  ],
  "author": "ipfaffen",
  "license": "MIT"
}
